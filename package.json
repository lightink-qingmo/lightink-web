{
  "name": "lightink-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "startpm2": "pm2 start ecosystem.config.js --env production",
