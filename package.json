    "docker:push": "docker push light-web/next-learn"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "axios": "^0.18.0",
    "cacheable-response": "^1.1.0",
    "express": "^4.17.1",
    "less": "^3.9.0",
    "lodash": "^4.17.11",
    "@material-ui/core": "latest",
    "@material-ui/styles": "latest",
    "clsx": "latest",
    "next": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
